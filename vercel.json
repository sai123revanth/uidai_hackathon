{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chatmodel",
      "dest": "/api/chatmodel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}